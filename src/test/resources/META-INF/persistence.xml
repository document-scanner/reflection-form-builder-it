    <class>richtercloud.reflection.form.builder.jpa.entities.EntityC</class>
    <class>richtercloud.reflection.form.builder.jpa.entities.EntityD</class>
    <class>richtercloud.reflection.form.builder.jpa.entities.EntityE</class>
